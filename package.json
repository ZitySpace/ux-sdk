{
  "name": "@zityspace/ux-sdk",
  "version": "0.1.0",
  "description": "ZitySpace UX SDK",
  "author": "ZitySpace",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "NODE_ENV=development rollup -c -w --bundleConfigAsCjs",
    "build": "NODE_ENV=production rollup -c --bundleConfigAsCjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint '*.{js,json}' 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix '*.{js,json}' 'src/**/*.{ts,tsx}'",
    "prettier": "prettier -w src .github",
    "prettier:check": "prettier --check src .github",
    "dry-run-release": "semantic-release --dry-run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/components/index.d.ts"
    },
    "./atoms": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/atoms/index.d.ts"
    },
    "./hooks": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "dist/components"
      ],
      "atoms": [
        "dist/atoms"
      ],
      "hooks": [
        "dist/hooks"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^1.0.5",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-webpack5": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tailwindcss/container-queries": "^0.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^20.1.7",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "autoprefixer": "^10.4.1",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "immer": "^9.0.12",
    "jest": "^27.4.5",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.23",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.9.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^19.0.3",
    "storybook": "^7.0.12",
    "tailwindcss": "^3.2.4",
    "ts-deepmerge": "^2.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "webpack": "5.65.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.12",
    "@zityspace/fabric": "6.0.0-beta1",
    "@zityspace/react-annotate": "0.12.0-beta.8",
    "ace-builds": "^1.21.1",
    "echarts": "^5.3.2",
    "echarts-gl": "^2.0.9",
    "jotai": "^2.1.0",
    "rc-slider": "^10.1.0",
    "rc-tooltip": "^5.2.2",
    "rc-util": "^5.30.0",
    "react-ace": "^10.1.0",
    "react-resize-detector": "^9.1.0",
    "react-toastify": "^8.1.1",
    "zustand": "^4.1.1"
  }
}
